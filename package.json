{
  "name": "nav-du",
  "private": true,
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "node scripts/dev-all.cjs",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium-desktop",
    "worker:favicon:typecheck": "tsc -p workers/favicon/tsconfig.json --noEmit",
    "worker:favicon:dev": "wrangler dev --config workers/favicon/wrangler.toml",
    "worker:favicon:deploy": "wrangler deploy --config workers/favicon/wrangler.toml",
    "deploy:personal": "npm run build && cp wrangler-personal.toml wrangler.toml && wrangler pages deploy dist --project-name nav-du --branch main",
    "deploy:demo": "npm run build && cp wrangler-demo.toml wrangler.toml && wrangler pages deploy dist --project-name demo-nav-du --branch main"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "fuse.js": "^7.1.0",
    "js-yaml": "^4.1.1",
    "pinyin-pro": "^3.27.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260114.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wrangler": "^4.59.1"
  }
}
